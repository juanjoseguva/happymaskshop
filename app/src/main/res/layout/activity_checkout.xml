<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/checkoutImage"
        android:layout_width="208dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/wood_sign" />

    <ImageView
        android:id="@+id/checkoutImage2"
        android:layout_width="208dp"
        android:layout_height="110dp"
        android:layout_marginBottom="108dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/wood_sign" />

    <ImageView
        android:id="@+id/checkoutImage3"
        android:layout_width="199dp"
        android:layout_height="110dp"
        android:layout_marginStart="208dp"
        android:layout_marginBottom="108dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/wood_sign" />

    <ImageView
        android:id="@+id/backImage"
        android:layout_width="200dp"
        android:layout_height="111dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/checkoutImage"
        app:srcCompat="@drawable/wood_sign" />

    <Button
        android:id="@+id/checkoutButton"
        android:layout_width="155dp"
        android:layout_height="80dp"
        android:backgroundTint="#00FFFFFF"
        android:text="@string/checkout"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/checkoutImage"
        app:layout_constraintEnd_toEndOf="@+id/checkoutImage"
        app:layout_constraintStart_toStartOf="@+id/checkoutImage"
        app:layout_constraintTop_toTopOf="@+id/checkoutImage" />

    <Button
        android:id="@+id/cancelOrderButton"
        android:layout_width="155dp"
        android:layout_height="80dp"
        android:backgroundTint="#00FFFFFF"
        android:text="@string/cancel_order"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/checkoutImage"
        app:layout_constraintEnd_toEndOf="@+id/checkoutImage2"
        app:layout_constraintStart_toStartOf="@+id/checkoutImage2"
        app:layout_constraintTop_toTopOf="@+id/checkoutImage2" />

    <Button
        android:id="@+id/barterButton"
        android:layout_width="155dp"
        android:layout_height="80dp"
        android:backgroundTint="#00FFFFFF"
        android:text="@string/barter"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/checkoutImage3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/checkoutImage2"
        app:layout_constraintTop_toTopOf="@+id/checkoutImage3" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="155dp"
        android:layout_height="80dp"
        android:backgroundTint="#00FFFFFF"
        android:text="@string/back"
        android:textColor="#000000"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/backImage"
        app:layout_constraintEnd_toEndOf="@+id/backImage"
        app:layout_constraintStart_toStartOf="@+id/backImage"
        app:layout_constraintTop_toTopOf="@+id/backImage" />

    <ScrollView
        android:layout_width="328dp"
        android:layout_height="287dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8">

        <TextView
            android:id="@+id/itemsSummary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Default Text"
            android:textSize="16sp" />
    </ScrollView>

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="410dp"
        android:layout_height="211dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/happymasklogo" />

</androidx.constraintlayout.widget.ConstraintLayout>