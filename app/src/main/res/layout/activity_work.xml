<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="406dp"
        android:layout_height="256dp"
        android:layout_marginStart="2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/happymasklogo" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="match_parent"
        android:layout_height="113dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView23"
        app:srcCompat="@drawable/long_wood_sign" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="match_parent"
        android:layout_height="125dp"
        android:layout_marginTop="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView30"
        app:srcCompat="@drawable/long_wood_sign" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="match_parent"
        android:layout_height="111dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        app:srcCompat="@drawable/long_wood_sign" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="match_parent"
        android:layout_height="99dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        app:srcCompat="@drawable/long_wood_sign" />

    <Button
        android:id="@+id/manageInventoryButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        android:text="Manage Inventory"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView28"
        app:layout_constraintTop_toBottomOf="@+id/imageView23"
        tools:layout_editor_absoluteX="200dp" />

    <Button
        android:id="@+id/manageOrdersButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        android:textSize="24sp"
        android:textColor="#000000"
        android:text="@string/manage_orders"
        app:layout_constraintBottom_toTopOf="@+id/imageView29"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        tools:layout_editor_absoluteX="-3dp" />

    <Button
        android:id="@+id/manageClientsButton"
        android:layout_width="match_parent"
        android:backgroundTint="#00FFFFFF"
        android:textSize="24sp"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:text="@string/manage_clients"
        app:layout_constraintBottom_toBottomOf="@+id/imageView29"
        app:layout_constraintTop_toTopOf="@+id/imageView29"
        tools:layout_editor_absoluteX="-3dp" />

    <Button
        android:id="@+id/endShiftButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        android:textColor="#000000"
        android:textSize="24sp"
        android:text="@string/clock_out"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        tools:layout_editor_absoluteX="-3dp" />

</androidx.constraintlayout.widget.ConstraintLayout>